<h1>ログイン</h1>

<%= form_with url: login_path, local: true do |f| %>
 <div class ="filed">
  <%= f.label :メールアドレス %><br>
  <%= f.text_field :email %>
</div>

 <div class ="filed">
  <%= f.label :パスワード %><br>
  <%= f.password_field :password %>
 </div>
 
 <div>
   <%= f.submit "ログイン" %>
 </div>
 <%= link_to "新規作成", new_user_path %>
<% end %>